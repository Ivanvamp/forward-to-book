{
  "manifest_version": 2,
  "name": "Ivanvamp Selector Tool",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "ivSelector.js" ],
	  "css": [ "ivSelector.css" ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
	"default_icon": "off.png"
  },
  "background": {
    "scripts": ["popup.js"]
  }
}